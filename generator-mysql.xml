<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">  
<generatorConfiguration>  
    <!-- 数据库驱动包位置 -->  
    <classPathEntry location="./mysql-connector-java-5.1.42.jar" />   
    <context id="DB2Tables" targetRuntime="MyBatis3">  
    		<property name="modelOnly" value="true"/>
    		<property name="javaFileEncoding" value="UTF-8" />
    		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
        <commentGenerator>  
            <property name="suppressAllComments" value="false" />  
        </commentGenerator>  
        <!-- 数据库链接URL、用户名、密码 -->  
         <jdbcConnection driverClass="com.mysql.jdbc.Driver" 
         	connectionURL="jdbc:mysql://172.16.5.163:3306/ywwl_wechat_test?characterEncoding=utf8"
          	userId="root" password="root">   
        </jdbcConnection>  
        <javaTypeResolver>  
            <property name="forceBigDecimals" value="false" />  
        </javaTypeResolver>  
        <!-- 生成模型的包名和位置 -->  
        <javaModelGenerator targetPackage="com.ywwl.wechat.po" 
        		targetProject="../../../../../../ywwl-wechat/ywwl-wechat-dal/src/main/java">  
            <property name="enableSubPackages" value="true" />  
            <property name="trimStrings" value="true" />  
        </javaModelGenerator>  
        <!-- 生成的映射文件包名和位置 -->  
        <sqlMapGenerator targetPackage="mybatis.mapper" 
        		targetProject="../../../../../../ywwl-wechat/ywwl-wechat-dal/src/main/resources">  
            <property name="enableSubPackages" value="true" />  
        </sqlMapGenerator>  
        <!-- 生成DAO的包名和位置 -->  
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.ywwl.wechat.dao" 
        		targetProject="../../../../../../ywwl-wechat/ywwl-wechat-dal/src/main/java">  
            <property name="enableSubPackages" value="true" />  
        </javaClientGenerator>  
        <!-- 要生成那些表(更改tableName和domainObjectName就可以) -->  
        <table tableName="demo" domainObjectName="Demo" 
        enableCountByExample="false" enableUpdateByExample="false" 
        enableDeleteByExample="false" enableSelectByExample="false" 
        selectByExampleQueryId="false" />  
    </context>  
</generatorConfiguration>  